<!DOCTYPE module SYSTEM "module.dtd">

<module name="motor_dynamics">
  <doc>
    <description>Motor dynamics identification</description>
  </doc>
  <settings>
    <dl_settings name="Motor Dynamics">
      <dl_setting var="actuator_idx" min="0" max="50" step="1" shortname="Act IDX" type="int8_t" handler="actuator_idx_handler"/>
      <dl_setting var="step_min" min="0" max="9600" step="1" shortname="Step Min" type="uint32_t" handler="step_min_handler"/>
      <dl_setting var="step_max" min="0" max="9600" step="1" shortname="Step Max" type="uint32_t" handler="step_max_handler"/>
      <dl_setting var="start_test" min="0" max="1" step="1" shortname="Start" type="uint8_t" handler="start_test_handler"/>
    </dl_settings>
  </settings>

  <header>
    <file name="motor_dynamics.h"/>
  </header>
  <init fun="motor_dynamics_init()"/>
  <makefile target="ap">
    <file name="motor_dynamics.c"/>
  </makefile>
</module>